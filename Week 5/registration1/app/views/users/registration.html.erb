<h1>Enter Registration Information Below</h1>

<form action="/users/registration" method="get">

  <label for="full_name">Full Name:</label><br>
  <input type="text" name="full_name" placeholder="Full Name"/><br>

  <label for="street_address">Street Address:</label><br>
  <input type="text" name="street_address" placeholder="Street Address"/><br>

  <label for="city">City:</label><br>
  <input type="text" name="city" placeholder="City"/><br>

  <label for="state">State:</label><br>
  <input type="text" name="state" placeholder="State"/><br>

  <label for="postal_code">Postal Code:</label><br>
  <input type="text" name="postal_code" placeholder="Postal Code"/><br>

  <label for="country">Country:</label><br>
  <input type="text" name="country" placeholder="Country"/><br>

  <label for="email">Email Address:</label><br>
  <input type="text" name="email" placeholder="Email Address"/><br>

  <label for="user_id">User ID:</label><br>
  <input type="text" name="user_id" placeholder="User ID" required/><br>

  <label for="password">Password:</label><br>
  <input type="text" name="password" placeholder="Password" required/><br><br>

  <input type="submit" value="Submit">
</form>
